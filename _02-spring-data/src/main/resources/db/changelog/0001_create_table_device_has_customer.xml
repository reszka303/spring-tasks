<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">
        <changeSet id="0001-01" author="javastart">
            <createTable tableName="device_has_customer">
                <column name="device_id" type="BIGINT">
                    <constraints nullable="false" foreignKeyName="fk_device_id" references="device(id)"/>
                </column>
                <column name="customer_id" type="BIGINT">
                    <constraints nullable="false" foreignKeyName="fk_customer_id" references="customer(id)"/>
                </column>
            </createTable>
        </changeSet>
</databaseChangeLog>